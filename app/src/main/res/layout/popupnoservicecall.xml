<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:visibility="gone"
    android:clickable="true"
    android:id="@+id/popupnoservicecall"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:background="@drawable/round_background_2_white"
        android:layout_margin="55dp"
        android:visibility="visible"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <TextView
            android:lineSpacingExtra="2dp"
            android:gravity="center"
            android:layout_marginTop="20dp"
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ServiceCall is cancelled by customer"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:layout_marginTop="25dp"
            android:background="@color/colorTransparent20"
            android:id="@+id/view33"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_constraintTop_toBottomOf="@+id/textView22" />

        <TextView
            android:layout_marginBottom="15dp"
            android:id="@+id/popupok"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginTop="17dp"
            android:text="Ok"
            android:gravity="center"
            android:textColor="@color/colorwork"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/view33"
            app:layout_constraintTop_toBottomOf="@+id/view33" />
    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.constraintlayout.widget.ConstraintLayout>