<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:behavior_hideable="false"
    app:behavior_peekHeight="@dimen/_1sdp"
    android:id="@+id/includebottomsheet"
    android:clickable="true"
    xmlns:app="http://schemas.android.com/apk/res-auto">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_gravity="bottom"
            android:background="@drawable/carnerradious"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">


            <View
                android:id="@+id/view33"
                android:layout_width="60dp"
                android:layout_height="6dp"
                android:layout_marginTop="20dp"
                android:background="#DDDDDD"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

            </View>


            <TextView
                android:id="@+id/textView51"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="34dp"
                android:fontFamily="@font/poppins_medium"
                android:text="Customer Location"
                android:textColor="#242424"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view33" />

            <TextView
                android:id="@+id/tv_customerlocation_arrivedlocation"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/poppins_regular"
                android:text="276 Carling Avenue Ottawa, Ontario, K1Z 7B5"
                android:textColor="#242424"
                android:textSize="13sp"
                android:maxLines="3"
                app:layout_constraintBottom_toBottomOf="@+id/textView53"
                app:layout_constraintEnd_toEndOf="@+id/textView51"
                app:layout_constraintStart_toStartOf="@+id/textView51"
                app:layout_constraintTop_toBottomOf="@+id/textView51" />

            <ImageView

                android:id="@+id/iv_navigatebutton"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView51"
                app:srcCompat="@drawable/round_grey" />

            <ImageView
                android:id="@+id/imageView35"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="@+id/iv_navigatebutton"
                app:layout_constraintEnd_toEndOf="@+id/iv_navigatebutton"
                app:layout_constraintStart_toStartOf="@+id/iv_navigatebutton"
                app:layout_constraintTop_toTopOf="@+id/iv_navigatebutton"
                app:srcCompat="@drawable/arrowloc" />

            <TextView
                android:id="@+id/textView53"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/poppins_medium"
                android:text="Navigate"
                android:textColor="@color/black"
                android:textSize="15sp"
                app:layout_constraintEnd_toEndOf="@+id/iv_navigatebutton"
                app:layout_constraintTop_toBottomOf="@+id/iv_navigatebutton" />

            <View
                android:id="@+id/view32"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="30dp"
                android:background="#F8F8F8"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView53">

            </View>


            <Button
                android:layout_marginTop="10dp"
                android:id="@+id/bt_arraivalAtPickup"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:background="@drawable/round_background_2"
                android:fontFamily="@font/poppins_semibold"
                android:paddingTop="@dimen/_5sdp"
                android:text="Arrived At Pickup"
                android:textAllCaps="false"
                android:textColor="@color/colorWhite"
                android:textSize="@dimen/_15ssp"
                app:layout_constraintBottom_toBottomOf="@+id/view32"
                app:layout_constraintEnd_toEndOf="@+id/textView53"
                app:layout_constraintStart_toStartOf="@+id/tv_customerlocation_arrivedlocation"
                app:layout_constraintTop_toBottomOf="@+id/tv_emergencycancel1"
                />

            <TextView
                android:background="@drawable/round_background_2_red"
                android:gravity="center"
                android:id="@+id/tv_emergencycancel1"
                android:layout_width="0dp"
                android:layout_height="46dp"
                android:textAllCaps="false"
                android:textColor="@color/colorWhite"
                android:textSize="@dimen/_14ssp"
                android:text="Emergency Cancel"
                android:fontFamily="@font/poppins_semibold"
                app:layout_constraintEnd_toEndOf="@+id/textView53"
                app:layout_constraintStart_toStartOf="@+id/tv_customerlocation_arrivedlocation"
                app:layout_constraintTop_toTopOf="@+id/view32"
                 />



        </androidx.constraintlayout.widget.ConstraintLayout>


<!--
    </androidx.constraintlayout.widget.ConstraintLayout>
-->



</FrameLayout>