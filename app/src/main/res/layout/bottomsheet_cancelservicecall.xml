    <androidx.constraintlayout.widget.ConstraintLayout


        android:id="@+id/mainBottomsheet"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_alignParentBottom="true"
        android:background="@drawable/carnerradious"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">


        <View
            android:id="@+id/view33"
            android:layout_width="60dp"
            android:layout_height="6dp"
            android:layout_marginTop="10dp"
            android:background="#DDDDDD"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </View>


        <TextView
            android:id="@+id/textView51"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/poppins_medium"
            android:text="Customer Location"
            android:textColor="#242424"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view33" />

        <TextView
            android:id="@+id/tv_customerlocationForCancelService"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:fontFamily="@font/poppins_regular"
            android:maxLines="3"
            android:text="276 Carling Avenue Ottawa, Ontario, K1Z 7B5"
            android:textColor="#242424"
            android:textSize="13sp"
            app:layout_constraintEnd_toEndOf="@+id/textView51"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/textView51"
            app:layout_constraintTop_toBottomOf="@+id/textView51"
            app:layout_constraintVertical_bias="0.266" />

        <ImageView
            android:visibility="visible"
            android:id="@+id/iv_cancelServicecall"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginEnd="30dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/textView53"
            app:layout_constraintTop_toTopOf="@+id/textView51"
            app:srcCompat="@drawable/round_grey" />

        <ImageView
            android:visibility="visible"
            android:id="@+id/cross_black"
            android:layout_width="16dp"
            android:layout_height="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_cancelServicecall"
            app:layout_constraintEnd_toEndOf="@+id/iv_cancelServicecall"
            app:layout_constraintStart_toStartOf="@+id/iv_cancelServicecall"
            app:layout_constraintTop_toTopOf="@+id/iv_cancelServicecall"
            app:srcCompat="@drawable/close" />

        <ImageView
            android:visibility="gone"
            android:id="@+id/iv_cancelServicecallblack"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="30dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.518"
            app:layout_constraintStart_toStartOf="@+id/textView53"
            app:layout_constraintTop_toTopOf="@+id/textView51"
            app:srcCompat="@drawable/round_black" />

        <ImageView
            android:visibility="gone"
            android:id="@+id/cross_white"
            android:layout_width="16dp"
            android:layout_height="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_cancelServicecallblack"
            app:layout_constraintEnd_toEndOf="@+id/iv_cancelServicecallblack"
            app:layout_constraintStart_toStartOf="@+id/iv_cancelServicecallblack"
            app:layout_constraintTop_toTopOf="@+id/iv_cancelServicecallblack"
            app:srcCompat="@drawable/whiteclose" />


        <TextView
            android:visibility="gone"
            android:id="@+id/textView53"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="10dp"
            android:fontFamily="@font/poppins_medium"
            android:text="Cancel Service"
            android:textColor="#242424"
            android:textSize="13sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.925"
            app:layout_constraintStart_toEndOf="@+id/tv_customerlocationForCancelService"
            app:layout_constraintTop_toBottomOf="@+id/iv_cancelServicecall" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/view31"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="#F8F8F8"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_customerlocationForCancelService">


            <Button
                android:id="@+id/tv_enterCustomerCode"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="13dp"
                android:background="@drawable/round_background_2"
                android:fontFamily="@font/poppins_semibold"
                android:paddingTop="@dimen/_5sdp"
                android:text="Enter Customer Code"
                android:textAllCaps="false"
                android:textColor="@color/colorWhite"
                android:textSize="@dimen/_15ssp"
                app:layout_constraintBottom_toBottomOf="@+id/view31"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_timer" />

            <TextView

                android:id="@+id/textView46"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:fontFamily="@font/poppins_regular"
                android:text="Customer Greet Timer"
                android:textColor="#919191"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/view31" />

            <ImageView
                android:id="@+id/iv_timerview"
                android:layout_width="46dp"
                android:layout_height="46dp"
                android:padding="13dp"
                android:layout_marginStart="10dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView46"
                app:layout_constraintStart_toEndOf="@+id/textView46"
                app:layout_constraintTop_toTopOf="@+id/textView46"
                app:srcCompat="@drawable/information" />


            <TextView
                android:id="@+id/tv_timer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="00:05"
                android:textColor="@color/black"
                android:textSize="17sp"
                app:layout_constraintEnd_toEndOf="@+id/textView46"
                app:layout_constraintHorizontal_bias="0.489"
                app:layout_constraintStart_toStartOf="@+id/textView46"
                app:layout_constraintTop_toBottomOf="@+id/textView46" />


        </androidx.constraintlayout.widget.ConstraintLayout>




    </androidx.constraintlayout.widget.ConstraintLayout>


<!--
    </androidx.constraintlayout.widget.ConstraintLayout>
-->



</FrameLayout>